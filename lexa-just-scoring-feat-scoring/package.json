    "bowser": "^2.9.0",
    "classnames": "^2.2.6",
    "css-circle-menu": "file:local-dependencies/CSS-Circle-Menu-master",
    "date-fns": "^2.15.0",
    "diff": "git+https://github.com/klister10/jsdiff.git",
    "dotenv-webpack": "^1.7.0",
    "double-metaphone": "^1.0.2",
