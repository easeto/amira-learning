{
  "name": "lexa-studentapp",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production && aws s3 sync s3://codepipeline-us-east-1-unity-builds/Bilingual/ ./dist/UnityBuild",
    "build-staging": "rm -rf dist && npm run install-staging && npm run build",
    "unitydist": "cp -r UnityBuild ./dist",
    "dev": "webpack --mode development",
    "start": "webpack-dev-server --progress --colors --mode development",
    "install": "npm run getbilingualunity && npm run install-prod",
    "install-staging": "npm run getstagingcreds && npm run getstagingenv && npm run getbilingualunity",
    "install-prod": "npm run getcreds && npm run getenv",
    "getcreds": "aws s3 cp s3://codepipeline-us-east-1-amira-creds/aws-exports-sis.js ./src/aws-exports.js",
    "getstagingcreds": "aws s3 cp s3://codepipeline-us-east-1-amira-creds/aws-exports-stage.js ./src/aws-exports.js",
    "getenv": "aws s3 cp s3://codepipeline-us-east-1-amira-creds/amira.env ./src/amira.env",
    "getstagingenv": "aws s3 cp s3://codepipeline-us-east-1-amira-creds/amira-staging.env ./src/amira.env",
    "getunity": "aws s3 sync s3://codepipeline-us-east-1-unity-builds/UnityBuild/ ./UnityBuild",
    "getbilingualunity": "aws s3 sync s3://codepipeline-us-east-1-unity-builds/Bilingual/ ./UnityBuild",
    "publish-staging-env": "aws s3 cp ./src/amira.env s3://codepipeline-us-east-1-amira-creds/amira-staging.env",
    "publish-prod-env": "aws s3 cp ./src/amira.env s3://codepipeline-us-east-1-amira-creds/amira.env",
    "invalidate-staging": "aws cloudfront create-invalidation --distribution-id E2Z8VW3HK0KX40 --paths /index.html",
    "invalidate-prod": "aws cloudfront create-invalidation --distribution-id E3KKVMBRKQ69E7 --paths /index.html",
    "build-without-unity": "rm -rf dist && webpack --mode production",
    "copy-to-staging": "cd dist && aws s3 sync . s3://amira-codebuild-deployment && aws s3 cp ./index.html s3://amira-codebuild-deployment --expires 0 --cache-control \"no-cache, no-store, must-revalidate\"",
    "copy-to-production": "cd dist && aws s3 sync . s3://amira-master-branch && aws s3 cp ./index.html s3://amira-master-branch --expires 0 --cache-control \"no-cache, no-store, must-revalidate\"",
    "build-and-deploy-to-staging": "npm run install-staging && npm run build-without-unity && npm run copy-to-staging && npm run invalidate-staging",
    "build-and-deploy-to-prod": "npm run install-prod && npm run build-without-unity && npm run copy-to-production && npm run invalidate-prod",
    "invalidate-test-site": "aws cloudfront create-invalidation --distribution-id E2MJTMQTJ6OBID --paths /index.html",
    "copy-to-test-site": "cd dist && aws s3 sync . s3://amira-dyslexia-demo",
    "build-and-deploy-to-test-site": "npm run install-staging && npm run build-without-unity && npm run copy-to-test-site && npm run invalidate-test-site"
  },
  "author": "Amira Learning",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
